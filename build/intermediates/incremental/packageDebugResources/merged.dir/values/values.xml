<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="color1">#dfba69</color>
    <color name="colorBlack">#000000</color>
    <color name="colorButtonBg">#4e515e</color>
    <color name="colorDarkIndigo">#303f9f</color>
    <color name="colorFbColorBtnBgConfirm">#ffffff</color>
    <color name="colorFbColorBtnBgNegative">#ffffff</color>
    <color name="colorFbColorBtnBgPositive">#ffffff</color>
    <color name="colorFbColorBtnTextConfirm">#4b4f56</color>
    <color name="colorFbColorBtnTextNegative">#4b4f56</color>
    <color name="colorFbColorBtnTextPositive">#4b4f56</color>
    <color name="colorFbColorCtrlBtnTopLine">#dddee0</color>
    <color name="colorFbColorDialogBgBodyMessage">#ffffff</color>
    <color name="colorFbColorDialogBgControllerPanel">#ffffff</color>
    <color name="colorFbColorDialogBgHeader">#f6f7f9</color>
    <color name="colorFbColorDialogRoundShapeBg">#ffffff</color>
    <color name="colorFbColorDialogRoundStroke">#c5c7c9</color>
    <color name="colorFbColorDialogTextBodyMessage">#1d2129</color>
    <color name="colorFbColorDialogTitle">#4b4f56</color>
    <color name="colorFbColorHeaderBelowLine">#dddee0</color>
    <color name="colorHalfBlack">#5d5d5d</color>
    <color name="colorIndigo">#3f51B5</color>
    <color name="colorLightHalfBlack">#9d9d9d</color>
    <color name="colorOrange">#ff5412</color>
    <color name="colorOrangeBtnBgConfirm">#ffffff</color>
    <color name="colorOrangeBtnBgNegative">#ffffff</color>
    <color name="colorOrangeBtnBgPositive">#ffffff</color>
    <color name="colorOrangeBtnTextConfirm">#ff5412</color>
    <color name="colorOrangeBtnTextNegative">#ff5412</color>
    <color name="colorOrangeBtnTextPositive">#ff5412</color>
    <color name="colorOrangeCtrlBtnTopLine">#00ffffff</color>
    <color name="colorOrangeDialogBgBodyMessage">#ffffff</color>
    <color name="colorOrangeDialogBgControllerPanel">#ffffff</color>
    <color name="colorOrangeDialogBgHeader">#ff5412</color>
    <color name="colorOrangeDialogRoundShapeBg">#ffffff</color>
    <color name="colorOrangeDialogRoundStroke">#00ffffff</color>
    <color name="colorOrangeDialogTextBodyMessage">#5d5d5d</color>
    <color name="colorOrangeDialogTitle">#ffffff</color>
    <color name="colorOrangeHeaderBelowLine">#00ffffff</color>
    <declare-styleable name="AttrsStyleableDialog"><attr format="reference" name="AttrsDialogRoundStroke"/><attr format="reference" name="AttrsDialogRoundShapeBg"/><attr format="reference" name="AttrsDialogHeaderBelowLine"/><attr format="reference" name="AttrsDialogCtrlBtnTopLine"/><attr format="reference" name="AttrsDialogHeaderBg"/><attr format="reference" name="AttrsDialogTitle"/><attr format="reference" name="AttrsDialogTextBodyMessage"/><attr format="reference" name="AttrsDialogBgBodyMessage"/><attr format="reference" name="AttrsDialogBgControllerPanel"/><attr format="reference" name="AttrsDialogBtnConfirm"/><attr format="reference" name="AttrsDialogBtnNegative"/><attr format="reference" name="AttrsDialogBtnPositive"/></declare-styleable>
    <dimen name="dialog_radius">6dp</dimen>
    <dimen name="dialog_text_size_button">16sp</dimen>
    <dimen name="dialog_text_size_message">16sp</dimen>
    <dimen name="dialog_text_size_title">20sp</dimen>
    <string name="app_name">Library Dialog</string>
    <style name="StyleDialogFbColorBgBodyMessage">
        <item name="android:background">@color/colorFbColorDialogBgBodyMessage</item>
    </style>
    <style name="StyleDialogFbColorBtnConfirm">
        <item name="android:buttonStyle">?android:attr/borderlessButtonStyle</item>
        <item name="android:textColor">@color/colorFbColorBtnTextConfirm</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:background">@color/colorFbColorBtnBgConfirm</item>
    </style>
    <style name="StyleDialogFbColorBtnNegative">
        <item name="android:buttonStyle">?android:attr/borderlessButtonStyle</item>
        <item name="android:textColor">@color/colorFbColorBtnTextNegative</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:background">@color/colorFbColorBtnBgNegative</item>
    </style>
    <style name="StyleDialogFbColorBtnPositive">
        <item name="android:buttonStyle">?android:attr/borderlessButtonStyle</item>
        <item name="android:textColor">@color/colorFbColorBtnTextPositive</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:background">@color/colorFbColorBtnBgPositive</item>
    </style>
    <style name="StyleDialogFbColorCtrlBtnTopLine">
        <item name="android:background">@color/colorFbColorCtrlBtnTopLine</item>
    </style>
    <style name="StyleDialogFbColorHeaderBelowLine">
        <item name="android:background">@color/colorFbColorHeaderBelowLine</item>
    </style>
    <style name="StyleDialogFbColorTextBodyMessage">
        <item name="android:textColor">@color/colorFbColorDialogTextBodyMessage</item>
        <!--<item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>-->
        <item name="android:textSize">@dimen/dialog_text_size_message</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="StyleDialogFbColorTitle">
        <item name="android:textColor">@color/colorFbColorDialogTitle</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">@dimen/dialog_text_size_title</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="StyleDialogOrangeBgBodyMessage">
        <item name="android:background">@color/colorOrangeDialogBgBodyMessage</item>
    </style>
    <style name="StyleDialogOrangeBtnConfirm">
        <item name="android:buttonStyle">?android:attr/borderlessButtonStyle</item>
        <item name="android:textColor">@color/colorOrangeBtnTextConfirm</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:background">@color/colorOrangeBtnBgConfirm</item>
    </style>
    <style name="StyleDialogOrangeBtnNegative">
        <item name="android:buttonStyle">?android:attr/borderlessButtonStyle</item>
        <item name="android:textColor">@color/colorOrangeBtnTextNegative</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:background">@color/colorOrangeBtnBgNegative</item>
    </style>
    <style name="StyleDialogOrangeBtnPositive">
        <item name="android:buttonStyle">?android:attr/borderlessButtonStyle</item>
        <item name="android:textColor">@color/colorOrangeBtnTextPositive</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:background">@color/colorOrangeBtnBgPositive</item>
    </style>
    <style name="StyleDialogOrangeCtrlBtnTopLine">
        <item name="android:background">@color/colorOrangeCtrlBtnTopLine</item>
    </style>
    <style name="StyleDialogOrangeHeaderBelowLine">
        <item name="android:background">@color/colorOrangeHeaderBelowLine</item>
    </style>
    <style name="StyleDialogOrangeTextBodyMessage">
        <item name="android:textColor">@color/colorOrangeDialogTextBodyMessage</item>
        <!--<item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>-->
        <item name="android:textSize">@dimen/dialog_text_size_message</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="StyleDialogOrangeTitle">
        <item name="android:textColor">@color/colorOrangeDialogTitle</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">@dimen/dialog_text_size_title</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="ThemeDialogFbColor" parent="@android:style/Theme.Dialog">
        <item name="AttrsDialogRoundStroke">@color/colorFbColorDialogRoundStroke</item>
        <item name="AttrsDialogRoundShapeBg">@color/colorFbColorDialogRoundShapeBg</item>
        <item name="AttrsDialogHeaderBg">@color/colorFbColorDialogBgHeader</item>
        <item name="AttrsDialogHeaderBelowLine">@style/StyleDialogFbColorHeaderBelowLine</item>
        <item name="AttrsDialogCtrlBtnTopLine">@style/StyleDialogFbColorCtrlBtnTopLine</item>
        <item name="AttrsDialogTitle">@style/StyleDialogFbColorTitle</item>
        <item name="AttrsDialogTextBodyMessage">@style/StyleDialogFbColorTextBodyMessage</item>
        <item name="AttrsDialogBgBodyMessage">@style/StyleDialogFbColorBgBodyMessage</item>
        <item name="AttrsDialogBgControllerPanel">@color/colorFbColorDialogBgControllerPanel</item>
        <item name="AttrsDialogBtnConfirm">@style/StyleDialogFbColorBtnConfirm</item>
        <item name="AttrsDialogBtnNegative">@style/StyleDialogFbColorBtnNegative</item>
        <item name="AttrsDialogBtnPositive">@style/StyleDialogFbColorBtnPositive</item>
    </style>
    <style name="ThemeDialogOrange" parent="@android:style/Theme.Dialog">
        <item name="AttrsDialogRoundStroke">@color/colorOrangeDialogRoundStroke</item>
        <item name="AttrsDialogRoundShapeBg">@color/colorOrangeDialogRoundShapeBg</item>
        <item name="AttrsDialogHeaderBg">@color/colorOrangeDialogBgHeader</item>
        <item name="AttrsDialogHeaderBelowLine">@style/StyleDialogOrangeHeaderBelowLine</item>
        <item name="AttrsDialogCtrlBtnTopLine">@style/StyleDialogOrangeCtrlBtnTopLine</item>
        <item name="AttrsDialogTitle">@style/StyleDialogOrangeTitle</item>
        <item name="AttrsDialogTextBodyMessage">@style/StyleDialogOrangeTextBodyMessage</item>
        <item name="AttrsDialogBgBodyMessage">@style/StyleDialogOrangeBgBodyMessage</item>
        <item name="AttrsDialogBgControllerPanel">@color/colorOrangeDialogBgControllerPanel</item>
        <item name="AttrsDialogBtnConfirm">@style/StyleDialogOrangeBtnConfirm</item>
        <item name="AttrsDialogBtnNegative">@style/StyleDialogOrangeBtnNegative</item>
        <item name="AttrsDialogBtnPositive">@style/StyleDialogOrangeBtnPositive</item>
    </style>
</resources>